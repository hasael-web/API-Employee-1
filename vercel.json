{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/users/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
"rewrites": [
    {
      "source": "/(.*)",
      "destination": "/server.js"
    }
  ],
  "public": {
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/$1"
      }
    ]
}
